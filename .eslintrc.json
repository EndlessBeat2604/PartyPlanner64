{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceType": "script"
    },
    "rules": {
        "no-const-assign": "warn",
        "no-this-before-super": "warn",
        "no-undef": "warn",
        "no-unreachable": "warn",
        "no-unused-vars": [1, {"vars": "all", "args": "none"}],
        "constructor-super": "warn",
        "valid-typeof": "warn"
    },
    "globals": {
        "PP64": false,
        "$$log": false,
        "$$hex": false,
        "$$number": false,
        "$$debug": false,
        "$MIPS": false,
        "$setting": false,
        "$viewType": false,
        "$actType": false,
        "$gameType": false,
        "$spaceType": false,
        "$spaceSubType": false,
        "$activationType": false,
        "$executionType": false,
        "$validationLevel": false,
        "JSZip": false,
        "saveAs": false,
        "SparkMD5": false,
        "MIPSInst": false,
        "MIPSAssem": false
    }
}